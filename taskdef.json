{
  "containerDefinitions": [
    {
      "name": "node-app",
      "image": "<IMAGE>",
      "memory": 512,
      "cpu": 128,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "node-app-td",
  "taskRoleArn": "arn:aws:iam::322926992319:role/ecsTaskExecutionRole"
}
